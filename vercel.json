{
  "version": 2,   
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
    "src": "/api/update-balance",
    "dest": "/api/server.js"
  },{
    "src": "/api/start-trade",
    "dest": "/api/server.js"
  },
    {
      "src": "/api/get-balance",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/create-payment",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/Verify_Payment",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/VerifyPayment",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/register",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/get-AccountDetails",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/Add-Account",
      "dest": "/api/server.js"
    } ,
    {
      "src": "/api/getWithdrawal",
      "dest": "/api/server.js"
    }   
  ]
}
