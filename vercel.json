{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",  
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/create-payment",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/Verify_Payment",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/register",
      "dest": "/api/server.js"
    }
  ]
}
